import java.awt.Color;
import wheelsunh.users.*;

public class Button extends TextBox
{
  public static int numDie1, numDie2;

  //------------------------Constructor-------------------------------------
  public Button( int x, int y)
  {
    setLocation( x, y );
    setFillColor( Color.RED );
    setSize( 40, 40 );
 
      
  }

} //End of Class